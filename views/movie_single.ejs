<!DOCTYPE html>
<html lang="en">
<head>
  
  <% include partials/head.ejs %>

</head>
<body>

	<% include partials/header.ejs %>

	<div id="hero-image-single" style="background-image: url('/images/<%= year.hero_image %>');">
		<h1 id="hero-title"><%= title %></h1>
	</div>


	<div class="content-padding clearfix">

		<div class="poster-wrapper">
  		<img src="/images/<%= year.poster %>" class="img-responsive">
  	</div>

  	<div class="description-wrapper">
  		<div class="main-characters">
  			<h2 class="movie-header">List Of Top 5 Songs Of <%= year_number %></h2>
  			<ol>
  			<% for (var i = 0; i < main_characters.length; i+=2) { %>
  				<li><a href="<%= main_characters[i+1] %>"><%= main_characters[i] %></a></li><br>
  			<% } %>
  			</ol>
  		</div>

  	</div>

	</div>



  <% include partials/footer.ejs %>

  <% include partials/scripts.ejs %>

</body>
</html>